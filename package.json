    "jest-skipped-reporter": "^0.0.5",
    "jest-slow-test-reporter": "^1.0.0",
    "jest-sonar": "0.2.12",
    "jest-without-globals": "^0.0.3",
    "jsonwebtoken": "8.5.1",
    "libsodium-wrappers": "0.7.10",
    "mocha": "10.1.0",
    "node-html-parser": "5.3.3",
    "node-notifier": "^10.0.1",
    "npm-run-all": "4.1.5",
    "passport": "0.6.0",
    "pinst": "3.0.0",
    "prettier": "2.6.2",
    "pretty-quick": "^3.1.3",
    "qs": "6.11.0",
    "semantic-release": "19.0.3",
    "ts-node": "10.7.0",
    "tslint-html-report": "^2.2.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.4",
    "typescript-json": "^3.3.22",
    "uuid": "8.3.2",
    "validate-branch-name": "^1.3.0"
  },
  "dependencies": {
    "@aws-sdk/client-athena": "^3.271.0",
    "@databases/pg": "^5.4.1",
    "@eaint/cdr-lib-types": "1.9.0",
    "@eaint/dataservices-lib-types": "^1.27.1",
    "@eaint/domain-lib-types": "1.0.0",
    "@eaint/lib-stubs": "1.55.1",
    "@eaint/logging": "1.2.3",
    "@nestjs/axios": "0.0.7",
    "@nestjs/common": "8.4.5",
    "@nestjs/core": "8.4.5",
    "@opensearch-project/opensearch": "^2.2.0",
    "@types/pg": "^8.6.6",
