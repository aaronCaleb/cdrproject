{
  "name": "e2e-sit",
  "author": "EAIntegration",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "description": "Local repository for CDR SIT End to End Testing.",
  "keywords": [
    "jest",
    "TypeScript",
    "matchers",
    "extend",
    "extended",
    "test",
    "testing",
    "assertions",
    "eslint-formatter",
    "eslint-formatter-autolinkable-stylish",
    "prettier"
  ],
  "repository": {
    "type": "git",
    "url": "https://EAIntegration@dev.azure.com/EAIntegration/ea-cdr-e2e-api-test-automation/_git/ea-cdr-e2e-api-test-automation"
  },
  "main": "server.js",
  "types": "./types/index.d.ts",
  "jest": {
    "testEnvironment": "node"
  },
  "typeCoverage": {
    "atLeast": 85
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npx validate-branch-name"
    }
  },
  "private": true,
  "scripts": {
    "semantic-release": "semantic-release",
    "prepack": "run-s build",
    "build": "run-s build:tsc",
    "build:tsc": "run-s clean:lib build:commonjs build:es2015 build:types",
    "build:es2017": "tsc --module ES2017 --outDir lib-es2017 --project tsconfig.build.json",
    "format": "run-s lint:fix prettier",
    "test:watch": "yarn test:ea-cdr-e2e --watch",
    "tls_reject": "set NODE_TLS_REJECT_UNAUTHORIZED=0",
    "accessToken:get": "ts-node ./bearerTokens/get-access-token.ts",
    "bearerToken:get": "ts-node ./bearerTokens/get-bearerToken.ts",
    "postgreSQLServer:start": "ts-node ./src/utils/databaseUtils/postgresdb.ts",
    "eaIdGenerator:start": "ts-node ./src/utils/databaseUtils/eaId.got.ts",
    "testDataImport:start": "ts-node ./src/data/testDataImport.ts",
    "e2eTest:start": "yarn tls_reject && jest  --config ./jest-config.js --forceExit --maxWorkers=3 --colors --coverage",
    "dbServer:start": "ts-node ./src/utils/databaseUtils/postgrestdb.ts",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "lint": "run-s lint:eslint lint:prettier",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:prettier": "prettier . -c",
    "eslint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:fix": "run-s eslint:fix prettier",
    "prettier": "prettier --write .",
    "dataBase:start": "nodemon ./src/database/postgrest.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@eadigital/commitlint-config": "1.5.0",
    "@eadigital/eslint-config": "2.2.0",
    "@eadigital/tsconfig": "1.4.0",
    "@eaint/api-common": "^3.96.1",
    "@jest-performance-reporter/core": "2.1.2",
    "@nestjs/platform-express": "8.4.5",
    "@nestjs/testing": "8.4.5",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "9.0.1",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@stryker-mutator/core": "^6.3.1",
    "@stryker-mutator/jest-runner": "^6.3.1",
    "@stryker-mutator/typescript-checker": "^6.3.1",
    "@types/axios": "0.14.0",
    "@types/chai": "4.3.0",
    "@types/express": "4.17.13",
    "@types/he": "1.1.2",
    "@types/jest": "27.4.1",
    "@types/jest-json-schema": "^6.1.1",
    "@types/jquery": "^3.5.14",
    "@types/jsonwebtoken": "8.5.8",
    "@types/libsodium-wrappers": "0.7.9",
    "@types/mocha": "9.1.0",
    "@types/node": "^17.0.30",
    "@types/qs": "6.9.7",
    "@types/supertest": "2.0.12",
    "@types/uuid": "8.3.4",
    "axios": "0.27.1",
    "chai": "4.3.6",
    "chai-http": "4.3.0",
    "dotenv": "16.0.1",
    "enforce-branch-name": "^1.1.2",
    "eslint": "^8.26.0",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "expect-more": "1.2.0",
    "he": "1.2.0",
    "helmet": "^6.0.0",
    "hereby": "^1.6.4",
    "hpagent": "1.1.0",
    "husky": "8.0.1",
    "jest-chain": "1.1.6",
    "jest-circus-allure-environment": "^1.1.1",
    "jest-email-reporter": "^1.0.4",
    "jest-expect-message": "^1.1.3",
    "jest-extended": "^3.1.0",
    "jest-html-reporter": "3.5.0",
    "jest-html-reporters": "^3.1.4",
    "jest-jasmine2": "^29.5.0",
    "jest-json": "^2.0.0",
    "jest-json-schema": "^6.1.0",
    "jest-junit": "^13.2.0",
